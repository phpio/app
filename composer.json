{
  "name": "phpio/app",
  "description": "php framework",
  "keywords": ["php", "app", "restful", "slim"],
  "homepage": "https://github.com/phpio/app",
  "license": "MIT",
  "authors": [
    {"name": "Demjan Kaluzki", "email": "kaluzkidemjan@gmail.com"}
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=5.6.0",
    "phpio/spl": "dev-master",
    "php-di/php-di": "~5.0",
    "php-di/invoker": "~1.0",
    "doctrine/cache": "~1.0",
    "slim/slim": "~3.0"
  },
  "require-dev": {
    "phpunit/PHPUnit": "~5.0"
  },
  "autoload": {
    "psr-4": {
      "Phpio\\": "src"
    },
    "files": [
      "src/App/Route/functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Phpio\\": "tests"
    }
  }
}
